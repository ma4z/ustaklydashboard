<script type="text/javascript">
    // Load Time System
    var before_loadtime = new Date().getTime();
    window.onload = Pageloadtime;
    function Pageloadtime() {
      var aftr_loadtime = new Date().getTime();
      // Time calculating in seconds
      pgloadtime = (aftr_loadtime - before_loadtime) / 1000;
  
      document.getElementById("loadtime").innerHTML = pgloadtime + "s";
    }
  </script>
  <footer class="bg-black/10 md:ml-64 max-w-10xl fixed mx-auto inset-x-0 bottom-0 backdrop-blur-md">
    <div class="mx-auto container px-8 py-4 sm:flex sm:items-center sm:justify-between">
      <p class="text-left text-sm text-gray-400 sm:text-left">
        
      </p>
      <p class="text-right text-sm text-gray-400 sm:text-left mt-2 sm:mt-0">
        <code>v0.1.0-beta1 - Loaded in <span id="loadtime"></span></code>
      </p>
    </div>
  </footer>
  </body>
  </html>